{
  "source": {
    "include": ["controllers", "models", "utils"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template"
  },
  "plugins": ["plugins/markdown"]
}
